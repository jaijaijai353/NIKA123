{
  "name": "nika-backend",
  "version": "1.0.0",
  "description": "NIKA Backend API for dataset processing",
  "main": "server.ts",
  "scripts": {
    "start": "npx ts-node server.ts",
    "dev": "nodemon --watch \"./**/*.ts\" --exec \"npx ts-node server.ts\"",
    "build": "npx tsc",
    "clean": "rm -rf dist uploads/*.* db.sqlite",
    "lint": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^13.6.0",
    "fs-extra": "^11.2.0",
    "multer": "^1.4.5-lts.1",
    "papaparse": "^5.4.1",
    "sqlite3": "^5.1.6",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/papaparse": "^5.3.14",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0",
    "supertest": "^6.3.4"
  },
  "keywords": [
    "nika",
    "data",
    "analytics",
    "backend"
  ],
  "author": "Jai Narula",
  "license": "MIT"
}
